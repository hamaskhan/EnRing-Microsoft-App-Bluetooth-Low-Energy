        <ComboBox x:Name="comboBoxC3" ItemsSource="{x:Bind keys}" IsTextSearchEnabled="True" x:FieldModifier="public" HorizontalAlignment="Center" Canvas.Left="274" Canvas.Top="352" VerticalAlignment="Top" Width="160" Background="{StaticResource CustomAcrylicBrush3}" Height="28" Margin="0,0,0,0"/>

        <Button x:Name="App_select" Content="Select App" HorizontalAlignment="Left" Canvas.Left="12" Canvas.Top="73" VerticalAlignment="Center" Width="115">
            <Button.Flyout>
                <Flyout>
                    <GridView x:Name="gridView" SelectionMode="Single" IsItemClickEnabled="True" Canvas.Left="442" Canvas.Top="80" Width="400" ItemsSource="{x:Bind Appsdata,Mode=OneWay}" Height="200" Background="{StaticResource CustomAcrylicBrush3}" SelectionChanged="app_selected" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                        <GridView.ItemTemplate>
                            <DataTemplate x:DataType="local:Apps_class">
                                <StackPanel CornerRadius="1" Height="120" Width="110">
                                    <Image Source="{x:Bind Appicon}" Width="65" Height="65" Margin="6 6 6 0"/>
                                    <TextBlock Text="{x:Bind Appname}" Margin="6 2 6 0" Padding="0 6 0 0" TextAlignment="Center" TextTrimming="CharacterEllipsis" TextWrapping="Wrap" Style="{ThemeResource HeaderTextBlockStyle}" FontSize="16"/>
                                </StackPanel>
                            </DataTemplate>
                        </GridView.ItemTemplate>
                    </GridView>
                </Flyout>
            </Button.Flyout>
        </Button>
        <Button x:Name="Addapp" Content="Add new App" HorizontalAlignment="Left" Canvas.Left="12" Canvas.Top="108" VerticalAlignment="Top" Width="115" Height="32" Margin="0,0,0,0" Click="Add_app"/>
        <Slider x:Name="Sensitivity" Header="Sensitivity" Canvas.Left="38" Canvas.Top="554" Width="200" StepFrequency="0.01" Maximum="0.5" Minimum="0.01" Value="0.1" TickFrequency="0.01" TickPlacement="TopLeft" Foreground="White" HorizontalAlignment="Stretch" Margin="0,0,0,0" VerticalAlignment="Stretch"/>
        <TextBlock x:Name="Min" Canvas.Left="8" Canvas.Top="580" Text="min" TextWrapping="Wrap" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
        <TextBlock x:Name="Max" Canvas.Left="241" Canvas.Top="580" Text="max" TextWrapping="Wrap" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
    </Canvas>
</Page>
   