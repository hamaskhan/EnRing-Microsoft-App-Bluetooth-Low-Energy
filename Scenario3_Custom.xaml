        <Button x:Name="Clear" Content="Clear" HorizontalAlignment="Center" Canvas.Left="533" Canvas.Top="538" VerticalAlignment="Top" Width="115" Click="Clear_Click" Height="32"/>
        <ComboBox x:Name="comboBoxL1" ItemsSource="{x:Bind keys}" IsTextSearchEnabled="True" x:FieldModifier="public" HorizontalAlignment="Center" Canvas.Left="17" Canvas.Top="297" VerticalAlignment="Top" Width="160" Background="{StaticResource CustomAcrylicBrush3}" Height="28" Margin="0,0,0,0"/>
        <ComboBox x:Name="comboBoxL2" ItemsSource="{x:Bind keys}" IsTextSearchEnabled="True" x:FieldModifier="public" HorizontalAlignment="Left" Canvas.Left="17" Canvas.Top="326" VerticalAlignment="Center" Width="160" Background="{StaticResource CustomAcrylicBrush3}" Height="28"/>
        <ComboBox x:Name="comboBoxL3" ItemsSource="{x:Bind keys}" IsTextSearchEnabled="True" x:FieldModifier="public" HorizontalAlignment="Center" Canvas.Left="17" Canvas.Top="357" VerticalAlignment="Top" Width="160" Background="{StaticResource CustomAcrylicBrush3}" Height="28"/>
        <ComboBox x:Name="comboBoxR1" ItemsSource="{x:Bind keys}" IsTextSearchEnabled="True" x:FieldModifier="public" HorizontalAlignment="Center" Canvas.Left="533" Canvas.Top="296" VerticalAlignment="Top" Width="160" Background="{StaticResource CustomAcrylicBrush3}" Height="28"/>
        <ComboBox x:Name="comboBoxR2" ItemsSource="{x:Bind keys}" IsTextSearchEnabled="True" x:FieldModifier="public" HorizontalAlignment="Center" Canvas.Left="533" Canvas.Top="326" VerticalAlignment="Top" Width="160" Background="{StaticResource CustomAcrylicBrush3}" Height="28"/>
        <ComboBox x:Name="comboBoxR3" ItemsSource="{x:Bind keys}" IsTextSearchEnabled="True" x:FieldModifier="public" HorizontalAlignment="Center" Canvas.Left="533" Canvas.Top="356" VerticalAlignment="Top" Width="160" Background="{StaticResource CustomAcrylicBrush3}" Height="28"/>
        <ComboBox x:Name="comboBoxU1" ItemsSource="{x:Bind keys}" IsTextSearchEnabled="True" x:FieldModifier="public" HorizontalAlignment="Left" Canvas.Left="275" Canvas.Top="72" VerticalAlignment="Top" Width="160" Background="{StaticResource CustomAcrylicBrush3}" Height="28" Margin="0,0,0,0"/>
        <ComboBox x:Name="comboBoxU2" ItemsSource="{x:Bind keys}" IsTextSearchEnabled="True" x:FieldModifier="public" HorizontalAlignment="Left" Canvas.Left="275" Canvas.Top="102" VerticalAlignment="Top" Width="160" Background="{StaticResource CustomAcrylicBrush3}" Height="28" Margin="0,0,0,0"/>
        <ComboBox x:Name="comboBoxU3" ItemsSource="{x:Bind keys}" IsTextSearchEnabled="True" x:FieldModifier="public" HorizontalAlignment="Left" Canvas.Left="275" Canvas.Top="132" VerticalAlignment="Top" Width="160" Background="{StaticResource CustomAcrylicBrush3}" Height="28"/>
        <ComboBox x:Name="comboBoxD1" ItemsSource="{x:Bind keys}" IsTextSearchEnabled="True" x:FieldModifier="public" HorizontalAlignment="Left" Canvas.Left="274" Canvas.Top="518" VerticalAlignment="Center" Width="160" Background="{StaticResource CustomAcrylicBrush3}" Height="28"/>
        <ComboBox x:Name="comboBoxD2" ItemsSource="{x:Bind keys}" IsTextSearchEnabled="True" x:FieldModifier="public" HorizontalAlignment="Left" Canvas.Left="274" Canvas.Top="547" VerticalAlignment="Center" Width="160" Background="{StaticResource CustomAcrylicBrush3}" Height="28" Margin="0,0,0,0"/>
